<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>requireSubjectOnHighCase</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL( Priority , &quot;High&quot;) &amp;&amp; ISBLANK( Subject )</errorConditionFormula>
    <errorMessage>You must enter a subject on a High Priority Case</errorMessage>
</ValidationRule>
